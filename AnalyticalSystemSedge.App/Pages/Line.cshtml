@page
@model AnalyticalSystemSedge.App.Pages.LineModel
@{
}



<div>
    <canvas id="myChart"></canvas>
</div>



<script>
    const labels = [
        'Январь',
        'Февраль',
        'Март',
        'Апрель',
        'Май',
        'Июнь',
    ];

    const data = {
        labels: labels,
        datasets: [{
            label: 'Количество заявления на признание в соцобслуживании/пересмотре услуг',
            backgroundColor: 'rgb(255, 99, 132)',
            borderColor: 'rgb(255, 99, 132)',
            data: [0, 10, 5, 2, 20, 30, 45],
        }]
    };

    const config = {
        type: 'line',
        data: data,
        options: {}
    };
</script>


<script>
    const myChart = new Chart(
        document.getElementById('myChart'),
        config
    );
</script>



